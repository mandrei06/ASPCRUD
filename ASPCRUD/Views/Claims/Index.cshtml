@model List<ASPCRUD.Models.Domain.Claim>
@{
}

<h1>Claims</h1>

<table class="table">
    <thead>
        <tr>
            <th>Claim Id</th>
            <th>Description</th>
            <th>Status</th>
            <th>Date</th>
            <th>Vehicle Id</th>
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.ClaimId</td>
                <td>@claim.Description</td>
                <td>@claim.Status</td>
                <td>@claim.Date</td>
                <td>@claim.Vehicle_Id</td>
                <td> <a href="Claims/ViewClaim/@claim.ClaimId">View</a> </td>
            </tr>
        }
    </tbody>
</table>